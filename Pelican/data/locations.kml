---
<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document>
<name>Locations</name>
<open>1</open>
  <Folder>
    <open>1</open>
{% for place in pelican.data.places %}
{% assign key = place[0] %}
{% assign data = place[1] %}
<Placemark id="{{ key }}">
<name>{{ data.name }}</name>
<MultiGeometry>
{% for coordinate in data.location %}
<Point>
<coordinates>{{ coordinate }}</coordinates>
</Point>
{% endfor %}
</MultiGeometry>
</Placemark>
{% endfor %}
</Document>
</kml>
